{
  "manifest_version": 3,
  "name": "ChatGPT Model Styler",
  "version": "1.0",
  "description": "Customize ChatGPT model emojis and border styles.",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://*.chatgpt.com/*"],
      "js": ["contentScript.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
}
